{
  "name": "upload-js-full",
  "version": "0.4.0",
  "description": "Upload JavaScript SDK",
  "author": "Upload <hello@upload.io> (https://upload.io)",
  "license": "MIT",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/upload-io/upload-js-full",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upload-io/upload-js-full.git"
  },
  "bugs": {
    "url": "https://github.com/upload-io/upload-js-full/issues"
  },
  "files": [
    "*"
  ],
  "scripts": {
    "generate": "./generator.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "dist": "rm -rf dist && webpack",
    "prepack": "rm -f upload-js-full.tgz && npm run dist",
    "postpack": "[ ! -f upload-js-full-*.tgz ] || mv upload-js-full-*.tgz upload-js-full.tgz",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{ts,tsx}": [
      "bash -c \"tsc --noEmit\""
    ],
    "*/**/*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,json,css,html,md,yaml,yml}": [
      "prettier -c"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.2.1",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-return-types-object-literals": "1.0.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "7.0.4",
    "lint-staged": "10.5.1",
    "prettier": "2.8.1",
    "ts-loader": "9.2.2",
    "typescript": "4.4.4",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "2.5.2"
  }
}
